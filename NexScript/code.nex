$ This is a very useful piece of software

FUNC oopify(prefix) -> prefix + "oop"

FUNC join(elements, separator)
	SET result = ""
	SET len = LEN(elements)

	FOR i = 0 TO len THEN
		SET result = result + elements/i
		IF i != len - 1 THEN SET result = result + separator
	END

	RETURN result
END

FUNC map(elements, func)
	SET new_elements = []

	FOR i = 0 TO LEN(elements) THEN
		APPEND(new_elements, func(elements/i))
	END

	RETURN new_elements
END

ECHO("Greetings universe!")

FOR i = 0 TO 5 THEN
	ECHO(join(map(["l", "sp"], oopify), ", "))
END

